[user]
	name = Shusay Tanaka
	email = sy_tanaka@gmail.com

[color]
  ui=auto

[core]
	excludesfile = /Users/shusay/.gitignore
  editor=vim

[alias]
  st = status
  l = log
  mst = checkout master
  br = branch
  co = checkout
  aa = add -A
  cm = commit
  cmm = commit -m
  origin = org
  pl = pull
  ps = push

[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[ghq]
  root = ~/src
[url "git@github.com:"]
  insteadOf = https://github.com/
[pack]
  threads = 4
