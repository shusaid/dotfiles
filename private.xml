<?xml version="1.0"?>
<root>

	<item>
		<name>SX_XR1</name>
		<identifier>private.XR1</identifier>
		<autogen>--KeyOverlaidModifier-- KeyCode::JIS_KANA, KeyCode::VK_MODIFIER_EXTRA1, KeyCode::JIS_KANA</autogen>

		<!-- Cursor -->
		<autogen>
			__KeyToKey__
			KeyCode::S, ModifierFlag::EXTRA1,
			KeyCode::CURSOR_LEFT
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::F, ModifierFlag::EXTRA1,
			KeyCode::CURSOR_RIGHT
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::E, ModifierFlag::EXTRA1,
			KeyCode::CURSOR_UP
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::D, ModifierFlag::EXTRA1,
			KeyCode::CURSOR_DOWN
		</autogen>

		<!-- Winlike -->
		<autogen>
			__KeyToKey__
			KeyCode::L, ModifierFlag::EXTRA1,
			KeyCode::PAGEDOWN
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::O, ModifierFlag::EXTRA1,
			KeyCode::PAGEUP
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::I, ModifierFlag::EXTRA1,
			KeyCode::HOME
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::K, ModifierFlag::EXTRA1,
			KeyCode::END
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::J, ModifierFlag::EXTRA1,
			KeyCode::FORWARD_DELETE
		</autogen>

		<autogen>
			__KeyToKey__
			KeyCode::SEMICOLON, ModifierFlag::EXTRA1,
			KeyCode::ENTER
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::SPACE, ModifierFlag::EXTRA1,
			KeyCode::DELETE
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::X, ModifierFlag::EXTRA1,
			KeyCode::JIS_COLON
    </autogen>

    <!-- $% -->
    <autogen>
      __KeyToKey__
      KeyCode::C, ModifierFlag::EXTRA1,
      KeyCode::KEY_5, ModifierFlag::SHIFT_L
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::V, ModifierFlag::EXTRA1,
      KeyCode::KEY_4, ModifierFlag::SHIFT_L
    </autogen>
	</item>

	<item>
		<name>SX_XL1</name>
		<identifier>private.xl1</identifier>
		<autogen>--KeyOverlaidModifier-- KeyCode::JIS_EISUU, KeyCode::VK_MODIFIER_EXTRA2, KeyCode::JIS_EISUU</autogen>

		<!-- {}[] -->
		<autogen>
			__KeyToKey__
			KeyCode::K, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L,
			KeyCode::JIS_BRACKET_LEFT
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::L, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L,
			KeyCode::JIS_BRACKET_RIGHT
		</autogen>

		<autogen>
			__KeyToKey__
			KeyCode::K, ModifierFlag::EXTRA2,
			KeyCode::JIS_BRACKET_LEFT, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::L, ModifierFlag::EXTRA2,
			KeyCode::JIS_BRACKET_RIGHT, ModifierFlag::SHIFT_L
		</autogen>

		<!-- ()<> -->
		<autogen>
			__KeyToKey__
			KeyCode::J, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L,
			KeyCode::COMMA, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::I, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L,
			KeyCode::DOT, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::J, ModifierFlag::EXTRA2,
			KeyCode::KEY_8, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::I, ModifierFlag::EXTRA2,
			KeyCode::KEY_9, ModifierFlag::SHIFT_L
		</autogen>

		<!-- +- -->
		<autogen>
			__KeyToKey__
			KeyCode::COMMA, ModifierFlag::EXTRA2,
			KeyCode::SEMICOLON, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::DOT, ModifierFlag::EXTRA2,
			KeyCode::MINUS
		</autogen>

		<!-- symbol -->
		<autogen>
			__KeyToKey__
			KeyCode::JIS_ATMARK, ModifierFlag::EXTRA2,
			KeyCode::SLASH, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::P, ModifierFlag::EXTRA2,
			KeyCode::JIS_COLON, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::O, ModifierFlag::EXTRA2,
			KeyCode::SLASH
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::SEMICOLON, ModifierFlag::EXTRA2,
			KeyCode::MINUS, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>__KeyToKey__ KeyCode::JIS_COLON, ModifierFlag::EXTRA2, KeyCode::JIS_UNDERSCORE</autogen>

		<autogen>
			__KeyToKey__
			KeyCode::H, ModifierFlag::EXTRA2,
			KeyCode::KEY_7, ModifierFlag::SHIFT_L
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::N, ModifierFlag::EXTRA2,
			KeyCode::KEY_2, ModifierFlag::SHIFT_L
		</autogen>

		<!-- ~^ -->
		<autogen>
			__KeyToKey__
			KeyCode::U, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L,
			KeyCode::JIS_HAT
		</autogen>
		<autogen>
			__KeyToKey__
			KeyCode::U, ModifierFlag::EXTRA2,
			KeyCode::JIS_HAT, ModifierFlag::SHIFT_L
		</autogen>

		<!-- Â¥| -->
		<autogen>
			__KeyToKey__
			KeyCode::M, ModifierFlag::EXTRA2,
			KeyCode::JIS_YEN, ModifierFlag::OPTION_L
		</autogen>

		<!-- Mouse Keys -->
		<autogen>__KeyToKey__ KeyCode::E, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_UP</autogen>
		<autogen>__KeyToKey__ KeyCode::D, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_DOWN</autogen>
		<autogen>__KeyToKey__ KeyCode::S, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_LEFT</autogen>
		<autogen>__KeyToKey__ KeyCode::F, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_RIGHT</autogen>

		<autogen>__SimultaneousKeyPresses__ KeyCode::E, KeyCode::F, ModifierFlag::EXTRA2, KeyCode::VK_MOUSEKEY_DIAGONAL_NE</autogen>
		<autogen>__SimultaneousKeyPresses__ KeyCode::E, KeyCode::S, ModifierFlag::EXTRA2, KeyCode::VK_MOUSEKEY_DIAGONAL_NW</autogen>
		<autogen>__SimultaneousKeyPresses__ KeyCode::S, KeyCode::D, ModifierFlag::EXTRA2, KeyCode::VK_MOUSEKEY_DIAGONAL_SW</autogen>
		<autogen>__SimultaneousKeyPresses__ KeyCode::D, KeyCode::F, ModifierFlag::EXTRA2, KeyCode::VK_MOUSEKEY_DIAGONAL_SE</autogen>

		<autogen>__KeyToKey__ KeyCode::A, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_BUTTON_LEFT</autogen>
		<autogen>__KeyToKey__ KeyCode::Z, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_BUTTON_RIGHT</autogen>
		<autogen>__KeyToKey__ KeyCode::G, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_BUTTON_MIDDLE</autogen>
		<autogen>__KeyToKey__ KeyCode::T, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_SCROLL_DOWN</autogen>
		<autogen>__KeyToKey__ KeyCode::V, ModifierFlag::EXTRA2,  KeyCode::VK_MOUSEKEY_SCROLL_UP</autogen>
	</item>

	<item>
		<name>HHK_solving</name>
