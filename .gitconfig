[user]
	name = shusay
	email = shusay.t@gmail.com

[color]
  ui=auto

[core]
	excludesfile = /Users/shusay/.gitignore
  editor=vim

[alias]
  st = status
  l = log
  mst = checkout master
  br = branch
  co = checkout
  cm = commit
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
